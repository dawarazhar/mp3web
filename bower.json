{   "name":         "mp3web"
  , "title":        "Web MP3 Player and Media Library Manager"
  , "description":  "Web MP3 Player and Media Library Manager based on Node.js, Laravel and HTML5 + lightweight JavaScript MVC modeling on FrontEnd"
  , "version":      "1.0.2"
  , "main":         "index.php"
  , "authors": [
        "tbaltrushaitis@gmail.com"
    ]
  , "private":  true
  , "license":  "MIT"
  , "keywords": [
        "mp3"
      , "audio"
      , "player"
      , "laravel"
      , "media"
      , "library"
      , "storage"
      , "manager"
      , "web"
    ]
  , "moduleType": [
        "globals"
    ]
  , "homepage": "https://github.com/tbaltrushaitis/mp3web#readme"
  , "ignore": [
        "**/.*"
      , "animation"
      , "build"
      , "bower_components"
      , "bower_modules"
      , "dist"
      , "node_modules"
      , "docs"
      , "test"
      , "tests"
      , "laravel-*"
      , "webroot"
    ]
  , "dependencies": {
        "animate.css":          "~3.5.2"
      , "bootstrap":            "~3.3.7"
      , "bootstrap-tagsinput":  "~0.8.0"
      , "font-awesome":         "~4.7.0"
      , "html5shiv":            "~3.7.3"
      , "jquery":               "~2.2.4"
      , "jquery-tmpl":          "#b504c8afba"
      , "lodash":               "~4.17.2"
      , "normalize.css":        "~4.2.0"
      , "raty":                 "~2.7.1"
      , "respond":              "~1.4.2"
      , "requirejs":            "~2.3.1"
      , "underscore":           "~1.8.3"
    }
  , "devDependencies": {
    }
  , "resolutions": {
        "jquery": "~2.2.4"
    }
  , "outdatedDependencies": {
        "bxslider-4":   "bx-slider.js#^4.2.12"
    }
  , "overrides": {
        "admin-lte": {
            "main": [
                "dist/**/*.*"
              , "!**/index2.html"
              , "!**/*.less"
            ]
        }
      , "bootstrap": {
            "main": [
                "dist/**/*.*"
              , "!**/*.less"
              , "!**/*.map"
              , "!**/npm.js"
              , "!**/*.min.js"
            ]
        }
      , "font-awesome": {
            "main": [
                "fonts/*"
              , "css/*.css"
              , "!css/*.min.css"
            ]
        }
      , "jquery-tmpl": {
            "main": [
                "*.js"
            ]
        }
      , "raty": {
            "main": [
                "lib/**/*.*"
            ]
        }
      , "wdt-loading": {
            "main": [
                "wdtLoading.*"
              , "wdtLoading.js"
              , "wdtMarkup.html"
            ]
        }
    }
}
