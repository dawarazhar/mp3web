{
  "Player": {
      "_entity": "Audio_Player"
    , "_config": {
        "selector": "audio#player"
      , "random": null
      , "repeat": null
      , "volume": 1.00
      , "tracks": {
            "container": "#tracklist"
          , "selector":  "li a"
        }
      , "states": {
          "playing": {
              "action":   "Pause"
            , "icon":     "fa-pause"
            , "progress": "active"
            , "title":    "Pause"
          }
        , "paused": {
              "action":   "Resume"
            , "icon":     "fa-play"
            , "progress": ""
            , "title":    "Resume"
          }
        , "stopped": {
              "action":   "Play"
            , "icon":     "fa-play"
            , "progress": ""
            , "title":    "Play"
          }
        }
      }
    , "_data": {
        "instance": {}
      , "tracks": {
            "current": -1
          , "list": []
        }
      }
  }
}
