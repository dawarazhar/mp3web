{   "name":        "mp3web"
  , "title":       "Web MP3 Player and Media Library Manager"
  , "description": "Web MP3 Player and Media Library Manager based on Node.js, Laravel and HTML5 + lightweight JavaScript MVC modeling on FrontEnd"
  , "version":     "1.0.3"
  , "type":        "project"
  , "license":     "MIT"
  , "homepage":    "https://tbaltrushaitis.github.io/mp3web/"
  , "author": {
        "name":  "Baltrushaitis Tomas"
      , "email": "tbaltrushaitis@gmail.com"
      , "url":   "https://github.com/tbaltrushaitis"
    }
  , "authors": [
      {   "name":  "CoAuthor"
        , "email": "CoAuthor@gmail.com"
      }
    ]
  , "require": {
        "php":               ">=7.0.0"
      , "fideloper/proxy":   ">=4.0.0"
      , "laravel/framework": "5.5.*"
      , "laravel/tinker":    "~1.0"
    }
  , "require-disabled": {
        "php":                  ">=5.5.38"
      , "laravel/framework":    "~5.2.31"
      , "wisembly/elephant.io": "~3.0"
    }
  , "require-dev": {
      "phpunit/phpunit": "~6.0"
    }
  , "require-dev-disabled": {
        "filp/whoops":      "~2.0"
      , "fzaninotto/faker": "~1.4"
      , "mockery/mockery":  "~1.0"
      , "symfony/thanks":   "^1.0"
    }
  , "autoload": {
      "classmap": [
          "database/seeds"
        , "database/factories"
      ]
    , "psr-4": {
        "App\\": "app/"
      }
    }
  , "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    }
  , "extra": {
      "laravel": {
        "dont-discover": []
      }
    }
  , "scripts": {
      "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('../src/.env.rc', '.env');\""
      ]
    , "post-create-project-cmd": [
          "@php artisan -vvv --no-interaction key:generate"
        , "@php artisan -vvv --no-interaction preset none"
      ]
    , "post-install-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postInstall"
        , "@php artisan -vvv --no-interaction vendor:publish"
      ]
    , "post-update-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        , "@php artisan -vvv --no-interaction key:generate"
        , "@php artisan -vvv --no-interaction config:clear"
        , "@php artisan -vvv --no-interaction optimize"
      ]
    , "post-autoload-dump": [
          "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        , "@php artisan -vvv --no-interaction package:discover"
      ]
    }
  , "config": {
        "preferred-install": "dist"
      , "sort-packages": true
      , "optimize-autoloader": true
    }
  , "keywords": [
        "mp3"
      , "audio"
      , "player"
      , "laravel"
      , "media"
      , "library"
      , "storage"
      , "manager"
      , "web"
    ]
}
