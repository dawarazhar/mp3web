{   "name":         "mp3web"
  , "title":        "Web MP3 Player and Media Library Manager"
  , "description":  "Web MP3 Player and Media Library Manager based on Node.js, Laravel and HTML5 + lightweight JavaScript MVC modeling on FrontEnd"
  , "version":      "1.0.2"
  , "author": {
        "name":  "Baltrushaitis Tomas"
      , "email": "tbaltrushaitis@gmail.com"
      , "url":   "https://github.com/tbaltrushaitis"
    }
  , "license":  "MIT"
  , "private":  true
  , "homepage": "https://github.com/tbaltrushaitis/mp3web"
  , "website":  "http://mp3.gsm-center.com.ua/"
  , "readmeFilename":   "README.md"
  , "engines": {
        "node": ">=0.10.1"
      , "php":  ">=5.5.38"
    }
  , "main":     "gulpfile.js"
  , "scripts": {
        "test":  "gulp test"
      , "dev":   "gulp dev"
      , "build": "gulp build"
      , "prod":  "gulp --env=production"
    }
  , "repository": {
        "type": "git"
      , "url":  "git+https://github.com/tbaltrushaitis/mp3web.git"
    }
  , "keywords": [
        "mp3"
      , "audio"
      , "player"
      , "laravel"
      , "media"
      , "library"
      , "storage"
      , "manager"
      , "web"
    ]
  , "bugs": {
        "url": "https://github.com/tbaltrushaitis/mp3web/issues"
    }
  , "dependencies": {
    }
  , "devDependencies": {
        "bower":                "^1.7.9"
      , "dateformat":           "^2.0.0"
      , "del":                  "^2.2.2"
      , "gulp":                 "^3.9.1"
      , "gulp-add-src":         "^0.2.0"
      , "gulp-aglio":           "~0.0.10"
      , "gulp-changed":         "^3.1.0"
      , "gulp-chmod":           "^2.0.0"
      , "gulp-chown":           "^1.1.0"
      , "gulp-clean-css":       "^3.0.4"
      , "gulp-concat":          "^2.6.0"
      , "gulp-concat-css":      "^2.3.0"
      , "gulp-data":            "^1.2.1"
      , "gulp-directory-sync":  "^1.2.3"
      , "gulp-exec":            "^2.1.2"
      , "gulp-file-include":    "^1.0.0"
      , "gulp-filter":          "^5.0.0"
      , "gulp-gzip":            "^1.4.0"
      , "gulp-header":          "^1.8.8"
      , "gulp-if":              "^2.0.1"
      , "gulp-inject":          "^4.1.0"
      , "gulp-jscs":            "^4.0.0"
      , "gulp-jshint":          "^2.0.1"
      , "gulp-json-editor":     "^2.2.1"
      , "gulp-livereload":      "^3.8.1"
      , "gulp-newer":           "^1.2.0"
      , "gulp-rename":          "^1.2.2"
      , "gulp-replace":         "^0.5.4"
      , "gulp-sequence":        "^0.4.6"
      , "gulp-tar":             "^1.9.0"
      , "gulp-template":        "^4.0.0"
      , "gulp-uglify":          "^3.0.0"
      , "gulp-util":            "^3.0.8"
      , "gulp-watch":           "^4.3.9"
      , "jquery":               "^2.2.4"
      , "jsdom":                "^10.1.0"
      , "jshint":               "^2.9.3"
      , "jshint-stylish":       "^2.2.1"
      , "lodash":               "~4.17.2"
      , "main-bower-files":     "^2.13.1"
      , "merge-stream":         "^1.0.0"
      , "minimist":             "^1.2.0"
      , "nconf":                "^0.8.4"
      , "vinyl-paths":          "^2.1.0"
    }
  , "options": {
        "bower": {
            "debugging":    true
          , "paths": {
                "bowerDirectory":   "bower_modules"
              , "bowerrc":          ".bowerrc"
              , "bowerJson":        "bower.json"
            }
        }
      , "file": {
            "encoding": "utf8"
          , "flag":     "r"
        }
      , "reporting": {
            "err":    true
          , "stderr": true
          , "stdout": true
        }
      , "exec": {
            "continueOnError":  false
          , "pipeStdout":       false
        }
      , "watch": {
            "ignoreInitial":    false
          , "verbose":          true
          , "readDelay":        100
        }
      , "sync": {
            "printSummary": true
          , "nodelete":     true
        }
      , "uglify": {
            "compress": {
                "sequences":        true
              , "properties":       true
              , "dead_code":        true
              , "drop_debugger":    true
              , "unsafe":           false
              , "conditionals":     true
              , "comparisons":      true
              , "evaluate":         true
              , "booleans":         true
              , "loops":            true
              , "unused":           true
              , "hoist_funs":       true
              , "hoist_vars":       false
              , "if_return":        true
              , "join_vars":        true
              , "cascade":          true
              , "side_effects":     true
              , "warnings":         true
              , "global_defs":      {}
            }
          , "mangle":   false
          , "output": {
                "indent_start":     0
              , "indent_level":     4
              , "quote_keys":       false
              , "ascii_only":       false
              , "inline_script":    false
              , "width":            80
              , "max_line_len":     32000
              , "beautify":         false
              , "source_map":       null
              , "bracketize":       false
              , "comments":         false
              , "semicolons":       true
            }
        }
    }
}
